{
    "name": "http-inspector-ui",
    "version": "0.0.15",
    "description": "Web UI for http-inspector. Debug Node.js outgoing HTTP[S] requests",
    "keywords": [
        "http-inspector",
        "mitm",
        "devtools",
        "development",
        "network",
        "http",
        "https",
        "sniffer",
        "logger",
        "debugger",
        "network debugger"
    ],
    "license": "WTFPL",
    "author": "corporateanon <corporateanon@gmail.com>",
    "main": "index.js",
    "bin": {
        "http-inspector-ui": "./bin/http-inspector-ui.js"
    },
    "files": [],
    "scripts": {
        "watch:client": "parcel watch client/main.html -d client-dist",
        "watch:server": "tsc -watch -p tsconfig.server.json",
        "run:server": "nodemon ./server-dist/index.js",
        "dev": "concurrently npm:watch:client npm:watch:server npm:run:server",
        "build-storybook": "build-storybook",
        "lint": "eslint .",
        "prepublishOnly": "npm run build",
        "storybook": "start-storybook -p 6006",
        "test": "jest",
        "tester": "HTTP_INSPECTOR=ui node tester.js"
    },
